{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [ ">0.25%", "not ie 11", "not op_mini all"]
      }
    }]
  ],
  "plugins": [
    "external-helpers",
    ["module-alias", [
      { "src": "./shared", "expose": "shared" },
      { "src": "./src", "expose": "src" }
    ]]    
  ]
}